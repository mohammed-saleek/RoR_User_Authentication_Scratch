<!DOCTYPE html>
<html>
<head>
	<title>User Login</title>
</head>
<body>

	<center>
		<h4>Login</h4>
		<%= form_for :user, url: logging_path, method: :post do |f| %>
		<p>
			<%= f.label :email %><br>
			<%= f.text_field :email %>
		</p>
		<br>
		<p>
			<%= f.label :password %><br>
			<%= f.text_field :password %>
		</p>
		<br>
		<p>
			<%= f.submit "Login" %>
		</p>
		<% end %>
		<%= link_to 'Back',pages_home_path %>
	</center>

</body>
</html>